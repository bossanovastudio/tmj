tmj.controller('HomeController', function($rootScope, $location, $scope, $http, $sce, $compile, $routeParams) {
    
    $rootScope.pageName = 'homePage';

    $http.get('/user/is_signed_in.json')
    .then(function(data){
        if (data.data.user) {
            storage.set('user', data.data.user, {TTL: 3600000});
            $rootScope.user = data.data.user;
        }
    })
});
