tmj.controller('ProfileController', function($rootScope, $scope, $http, $sce, $compile, $routeParams) {

    if (!storage.get('user')) {
        $location.path('/login');
    }
    $scope.user = storage.get('user');

    $rootScope.pageName = $scope.user.username;
    $('body').addClass('edit-user');

});
