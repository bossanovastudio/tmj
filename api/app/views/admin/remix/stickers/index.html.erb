<section id="stickers" class="remix-sub">
  <div class="top-bar">
    <h1 class="title">Stickers</h1>
  </div>
    <div class="content">
        <%= link_to "Novo Sticker", new_admin_remix_sticker_path, class: 'button' %>
        <ul>
            <% @stickers.each do |sticker| %>
            <li><img src="<%= sticker.image.url %>" alt="">
            <%= link_to "Apagar", admin_remix_sticker_path(sticker), class: 'button', method: :delete, remote: true %>
            </li>
            <% end %>
        </ul>
    </div>
</section>
