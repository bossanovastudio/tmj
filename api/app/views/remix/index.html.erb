<div role="main">
  <div class="container">
    <div class="remix-container">
      <%= render "title" %>
      <section class="remix-composer">
        <button type="button" class="cancel">&times;</button>
        <div class="actions">
          <button type="button" class="next">
            <%= image_tag "remix/icon-arrow-right", alt: 'Próximo' %>
          </button>
          <button type="button" class="publish">Salvar</button>
          <button type="button" class="create-new">Criar um novo</button>
          <button type="button" class="start-over">
            <%= image_tag "remix/icon-eraser", alt: 'Começar novamente' %>
          </button>
          <a href="#" target="_blank" class="download">
            <%= image_tag "remix/icon-arrow-down", alt: 'Baixar' %>
          </a>
          <button type="button" class="remove">
            <%= image_tag "remix/icon-trash", alt: 'Remover' %>
          </button>
        </div>
        <div class="artboard">
          <div class="remix-canvas"></div>
          <div class="empty">Escolha uma categoria</div>
        </div>
        <div class="toolbox">
          <div class="toolbox-item toolbox-item-popup category on">
            <button type="button" class="toggler" title="Categoria">
              <%= image_tag "remix/icon-image", alt: 'Categoria' %>
            </button>
            <div class="popup">
              <div class="popup-holder">
                <div class="categories">
                  <div class="item take-photo">
                    <div>
                      <%= image_tag "remix/icon-camera", alt: 'Enviar foto' %>
                      <input type="file">
                    </div>
                  </div>
                </div>
                <div class="pictures">
                  <div class="item go-back">
                    <div>
                      <%= image_tag "remix/icon-arrow-left" %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="toolbox-item toolbox-item-colors color">
            <button type="button" class="toggler" title="Aquarela">
              <%= image_tag "remix/icon-watercolor", alt: 'Aquarela' %>
            </button>
          </div>
          <div class="toolbox-item toolbox-item-popup toolbox-item-popup-above toolbox-item-elements patterns">
            <button type="button" class="toggler" title="Backgrounds">
              <%= image_tag "remix/icon-background", alt: 'Backgrounds' %>
            </button>
            <div class="popup">
              <div class="popup-holder elements">
              </div>
            </div>
          </div>
          <div class="toolbox-item toolbox-item-effects effects">
            <button type="button" class="toggler" title="Efeitos">
              <%= image_tag "remix/icon-magic-wand", alt: 'Efeitos' %>
            </button>
          </div>
          <div class="toolbox-item toolbox-item-popup toolbox-item-popup-above toolbox-item-elements balloons">
            <button type="button" class="toggler" title="Balões">
              <%= image_tag "remix/icon-balloon", alt: 'Balões' %>
            </button>
            <div class="popup">
              <div class="popup-holder elements">
              </div>
            </div>
          </div>
          <div class="toolbox-item toolbox-item-popup toolbox-item-popup-above toolbox-item-elements stickers">
            <button type="button" class="toggler" title="Stickers">
              <%= image_tag "remix/icon-smile", alt: 'Stickers' %>
            </button>
            <div class="popup">
              <div class="popup-holder elements">
              </div>
            </div>
          </div>
          <div class="toolbox-item toolbox-item-popup toolbox-item-popup-above toolbox-item-texts">
            <button type="button" class="toggler" title="Texto">
              <%= image_tag "remix/icon-typography", alt: 'Texto' %>
            </button>
            <div class="popup">
              <div class="popup-holder options">
                <div class="color-palette">
                  <div class="holder">
                    <button type="button">
                      <!-- <div class="palette-item" style="background-color:#FBB040;"></div>
                      <div class="palette-item" style="background-color:#8DC63F;"></div>
                      <div class="palette-item" style="background-color:#00A79D;"></div>
                      <div class="palette-item" style="background-color:#27AAE1;"></div>
                      <div class="palette-item" style="background-color:#92278F;"></div>
                      <div class="palette-item" style="background-color:#231F20;"></div> -->
                      <%= image_tag "remix/icon-color-palette", alt: 'Paleta de cores' %>
                    </button>
                  </div>
                </div>
                <div class="styles">
                  <div class="styles-item g1" data-class="g1">
                    <span>G1</span>
                  </div>
                  <div class="styles-item g2" data-class="g2">
                    <span>G2</span>
                  </div>
                  <div class="styles-item m1" data-class="m1">
                    <span>M1</span>
                  </div>
                  <div class="styles-item m2" data-class="m2">
                    <span>M2</span>
                  </div>
                  <div class="styles-item p1" data-class="p1">
                    <span>P1</span>
                  </div>
                  <div class="styles-item p2" data-class="p2">
                    <span>P2</span>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <a class="btn btn-xs btn-default publish">Salvar</a>
        </div>
        <div class="share">
          <p class="call">
            Uau, seu remix ficou lindo!<br>
            Compartilhe para aparecer na Home!
          </p>
          <div class="networks">
            <%= link_to '', { :class => "btn btn-social btn-facebook", :target => "_blank", id: 'facebook_share_btn' } do %>
              <%= image_tag "remix/icon-facebook", alt: 'Facebook' %>
              Compartilhar no Facebook
            <% end %>
            <%= link_to '', { :class => "btn btn-social btn-twitter", :target => "_blank", id: 'twitter_share_btn' } do %>
              <%= image_tag "remix/icon-twitter", alt: 'Twitter' %>
              Compartilhar no Twitter
            <% end %>
            <!--<%= link_to '', { :class => "btn btn-social btn-tumblr", :target => "_blank", id: 'tumblr_share_btn' } do %>
              <%= image_tag "remix/icon-tumblr", alt: 'Tumblr' %>
              Compartilhar no Tumblr
            <% end %>-->
          </div>
        </div>
      </section>
      <%= render "gallery" %>
      <!-- <section class="remix-loader"></section> -->
    </div>
  </div>
</div>
